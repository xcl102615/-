package model.food;

import model.shop.Shop;
import project.model.BaseObject;

import java.util.List;

/***
 * 食品类
 */
public class Food extends BaseObject {
    private String shopId;
    //    店铺Id
    private String foodType;
    //  店铺食品分类
    private String type;
    //    平台类别
    private String image;
    //  食品图片
    private double price;
    //    如果商品没有其他规格时候的价格
    private List<FoodFormat>foodFormats;
    //规格
    private int monthSaleNum;
    //    月销售
    private int seleCount;
    //    销售总量
    private int list;
    //  排序方式

    public int getSeleCount() {
        return seleCount;
    }

    public void setSeleCount(int seleCount) {
        this.seleCount = seleCount;
    }

    public int getList() {
        return list;
    }

    public void setList(int list) {
        this.list = list;
    }

    public String getShopId() {
        return shopId;
    }
    public String getImage() {
        return image;
    }

    public void setImage(String image) {
        this.image = image;
    }

    public double getPrice() {
        return price;
    }

    public void setPrice(double price) {
        this.price = price;
    }

    public void setShopId(String shopId) {
        this.shopId = shopId;
    }

    public String getFoodType() {
        return foodType;
    }

    public void setFoodType(String foodType) {
        this.foodType = foodType;
    }

    public String getType() {
        return type;
    }

    public void setType(String type) {
        this.type = type;
    }

    public List<FoodFormat> getFoodFormats() {
        return foodFormats;
    }

    public void setFoodFormats(List<FoodFormat> foodFormats) {
        this.foodFormats = foodFormats;
    }

    public int getMonthSaleNum() {
        return monthSaleNum;
    }

    public void setMonthSaleNum(int monthSaleNum) {
        this.monthSaleNum = monthSaleNum;
    }
}
